// Floor material, 2012
// Material that is used as base for the floors and holes.
// Patrick Joos, Eduardo Hahn Paredes

material Floor 
{
    receive_shadows on
    technique
    {
        pass
        {
            shading phong
            texture_unit floor
            {
                texture floor_color.png
                tex_address_mode wrap
            }
        }        
    }
}
