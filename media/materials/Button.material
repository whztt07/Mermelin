// Button material, 2012
// Material that is used on the inside of the buttons.
// Eduardo Hahn Paredes

material Button 
{
    receive_shadows off
    technique
    {
        pass
        {
            ambient  0.7 0.0 0.0 1.0
            diffuse  0.5 0.0 0.0 1.0
            specular 0.4 0.0 0.0 1.0 30.0
        }
    }
}
